<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApp.Pages.Note">
  <ContentPage.Content>
    <StackLayout Padding="50">
      <Entry Text="{Binding NoteName, Mode=TwoWay}" />
      <Button Clicked="Button_Clicked" Text="Download data"/>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>